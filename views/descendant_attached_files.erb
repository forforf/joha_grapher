<div id="desc-info-attached_files">
<h2>All Attached Files</h2>
<% @desc_data.each do |field, node_data| %>
  <% node_data.each do |node_id, field_data| %>
    <% next unless field_data %>
    <% field_data.each do |att_name| %>
      <a href="javascript:void(0)" class="desc desc_attached_files" onclick="downloadAttachment('<%=node_id%>', '<%= att_name %>')"><%= att_name %></a>
    <% end %>
  <% end %>
<% end %>
